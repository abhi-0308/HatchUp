<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
         version="3.1">
         
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>com.backend.APP_Startup.SignupServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/SignupServlet</url-pattern>
    </servlet-mapping>
    
    
	<servlet>
    	<servlet-name>ProfileServlet</servlet-name>
    	<servlet-class>com.backend.APP_Startup.ProfileServlet</servlet-class>
	</servlet>

	<servlet-mapping>
    	<servlet-name>ProfileServlet</servlet-name>
    	<url-pattern>/ProfileServlet</url-pattern>
	</servlet-mapping>
	
	
	 <servlet>
        <servlet-name>EventRegistrationServlet</servlet-name>
    	<servlet-class>com.backend.APP_Startup.EventRegistrationServlet </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EventRegistrationServlet</servlet-name>
        <url-pattern>/EventRegistrationServlet </url-pattern>
    </servlet-mapping>
    
    
     <servlet>
        <servlet-name>EventRegistrationHandlerServlet</servlet-name>
    	<servlet-class>com.backend.APP_Startup.EventRegistrationHandlerServlet </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EventRegistrationHandlerServlet</servlet-name>
        <url-pattern>/EventRegistrationHandlerServlet </url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>TechSupportServlet</servlet-name>
    	<servlet-class>com.backend.APP_Startup.TechSupportServlet </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TechSupportServlet</servlet-name>
        <url-pattern>/TechSupportServlet </url-pattern>
    </servlet-mapping>

    

    </web-app>

